// Declare variables using var, let, and const
var name = "Sakthi";
let age = 19;
const country = "India";
console.log("Name:", name);
console.log("Age:", age);
console.log("Country:", country);

name = "deepika";
age = 19;

console.log("Updated Name:", name);
console.log("Updated Age:", age);
